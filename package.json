{
  "name": "mp3-to-svg",
  "version": "1.0.1",
  "description": "Takes in an audio file and outputs a SVG waveform",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "scripts": {
    "prepare": "npm run build",
    "lint": "eslint src/**",
    "build": "babel --presets es2015 src -d lib",
    "start": "node lib/cli",
    "dev": "./node_modules/.bin/babel-node src/cli",
    "test-cov": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "test-dev": "babel-node ./node_modules/.bin/_mocha spec/*spec.js --watch",
    "test": "babel-node ./node_modules/.bin/_mocha spec/*spec.js"
  },
  "bin": {
    "mp3-to-svg": "./lib/cli.js"
  },
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kdgm/mp3-to-svg.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Wouter Bin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^5.9",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.7.0",
    "glob": "^7.1.3",
    "isparta": "^4.0.0",
    "mocha": "^3.5.0"
  },
  "dependencies": {
    "rimraf": "^2.6.1",
    "svgo": "^1.0.3"
  }
}
